<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="654.0" prefWidth="891.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.lowman.creatpivotsheets.MainController">
   <children>
      <Button fx:id="pivotButton" layoutX="419.0" layoutY="506.0" mnemonicParsing="false" onAction="#onPivotButtonClick" text="PIVOT!" />
      <TextArea fx:id="textArea" layoutX="20.0" layoutY="14.0" prefHeight="369.0" prefWidth="858.0" />
      <Button fx:id="exitButton" layoutX="778.0" layoutY="569.0" mnemonicParsing="false" onAction="#exit" prefHeight="53.0" prefWidth="63.0" text="Exit" />
      <ImageView fx:id="imageView" fitHeight="240.0" fitWidth="747.0" layoutX="218.0" layoutY="399.0" pickOnBounds="true" preserveRatio="true" visible="false">
         <image>
            <Image url="@PIVOT.gif" />
         </image>
      </ImageView>
      <Button fx:id="excelButton" layoutX="783.0" layoutY="480.0" mnemonicParsing="false" onAction="#loadExcel" prefHeight="53.0" prefWidth="53.0" text="Excel" />
   </children>
</AnchorPane>
